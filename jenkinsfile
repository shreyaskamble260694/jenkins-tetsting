pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo '========executing build========'
            }
        }

        stage('package') {
            steps {
                echo '========executing package========'
            }
        }

        stage('testing_env') {
            steps {
                echo '========executing testing_env========'
            }
        }

        stage('production_env') {
            steps {
                echo '========executing production_env========'
            }
        }
    }
    post {
        always {
            echo '========always========'
        }
        success {
            echo '========pipeline executed successfully ======='
        }
        failure {
            echo '========pipeline execution failed========'
        }
    }
}
